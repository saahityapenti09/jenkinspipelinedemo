pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
     stage('build') {
            steps {
                echo 'Building'
            }
        } 
     stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
